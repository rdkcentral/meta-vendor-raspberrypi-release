From c77f9863897a1e0fc82c48405bad21de9bb54ae9 Mon Sep 17 00:00:00 2001
From: Ansu Mathew <ansu.mathew@rdkcentral.com>
Date: Thu, 13 Feb 2025 07:02:38 +0000
Subject: [PATCH] plugin change to remove IDictionary dependency temporary fix
 for build failure

---
 plugin/CobaltImplementation.cpp | 8 ++++++++
 plugin/CobaltJsonRpc.cpp        | 6 ++++++
 2 files changed, 14 insertions(+)

diff --git a/plugin/CobaltImplementation.cpp b/plugin/CobaltImplementation.cpp
index 8ae38e7..40959a3 100644
--- a/plugin/CobaltImplementation.cpp
+++ b/plugin/CobaltImplementation.cpp
@@ -18,7 +18,9 @@
 #include "Module.h"
 #include <interfaces/IMemory.h>
 #include <interfaces/IBrowser.h>
+#if 0
 #include <interfaces/IDictionary.h>
+#endif
 #include <interfaces/IFocus.h>
 
 extern "C" {
@@ -79,7 +81,9 @@ enum StateChangeCommand : uint16_t {
 class CobaltImplementation:
     public Exchange::IBrowser,
     public PluginHost::IStateControl,
+#if 0
     public Exchange::IDictionary,
+#endif
     public Exchange::IFocus
 {
 private:
@@ -649,6 +653,7 @@ public:
   };
 
   // IDictionary iface
+#if 0
   void Register(const string& nameSpace, struct IDictionary::INotification* sink) override  {  }
   void Unregister(const string& nameSpace, struct IDictionary::INotification* sink) override  {  }
   IIterator* Get(const string& nameSpace) const override { return nullptr; }
@@ -674,6 +679,7 @@ public:
     }
     return false;
   }
+#endif
 
   void OnConcealRequest() {
     // Device lifecycle tests from YTS expect 'suspend' behavior on 'conceal'
@@ -684,7 +690,9 @@ public:
   BEGIN_INTERFACE_MAP (CobaltImplementation)
   INTERFACE_ENTRY (Exchange::IBrowser)
   INTERFACE_ENTRY (PluginHost::IStateControl)
+#if 0
   INTERFACE_ENTRY (Exchange::IDictionary)
+#endif
   INTERFACE_ENTRY (Exchange::IFocus)
   END_INTERFACE_MAP
 
diff --git a/plugin/CobaltJsonRpc.cpp b/plugin/CobaltJsonRpc.cpp
index 6a70cd7..9ca6581 100644
--- a/plugin/CobaltJsonRpc.cpp
+++ b/plugin/CobaltJsonRpc.cpp
@@ -17,7 +17,9 @@
 */
 #include <interfaces/json/JsonData_Browser.h>
 #include <interfaces/json/JsonData_StateControl.h>
+#if 0
 #include <interfaces/IDictionary.h>
+#endif
 #include <core/Optional.h>
 
 #include "Cobalt.h"
@@ -203,6 +205,7 @@ uint32_t Cobalt::get_accessibility(JsonObject &response) const
   ASSERT(_cobalt != nullptr);
   uint32_t result = Core::ERROR_GENERAL;
 
+#if 0
   Exchange::IDictionary *dict(
     _cobalt->QueryInterface<Exchange::IDictionary>());
   if (dict == nullptr) {
@@ -220,6 +223,7 @@ uint32_t Cobalt::get_accessibility(JsonObject &response) const
     }
     dict->Release();
   }
+#endif
 
   return result;
 }
@@ -236,6 +240,7 @@ uint32_t Cobalt::set_accessibility(const JsonObject &param)
 
   if (param.IsSet()) {
     result = Core::ERROR_GENERAL;
+#if 0
     Exchange::IDictionary *dict(
       _cobalt->QueryInterface<Exchange::IDictionary>());
     if (dict == nullptr) {
@@ -253,6 +258,7 @@ uint32_t Cobalt::set_accessibility(const JsonObject &param)
       }
       dict->Release();
     }
+#endif
   }
   return result;
 }
